  "scripts": {
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "prebuild:prod": "rimraf dist",
    "build:platform:prod": "npm run build:backend:prod; npm run build:frontend:prod",
    "build:backend:prod": "nest build backend --webpack",
    "build:chain-observer:prod": "nest build chain-observer --webpack",
    "build:eth-payments-relayer:prod": "nest build eth-payments-relayer --webpack",
    "build:frontend:dev": "export NODE_ENV=development && npx webpack --config webpack-frontend.config.js --watch",
    "build:frontend:prod": "export NODE_ENV=production && npx webpack --config webpack-frontend.config.js",
    "start:backend:dev": "nest build backend --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:backend:prod": "npm run build:backend:prod && node dist/apps/backend/main",
    "start:chain-observer:dev": "nest build chain-observer --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:chain-observer:prod": "npm run build:chain-observer:prod && node dist/apps/chain-observer/main",
    "start:eth-payments-relayer:dev": "nest build eth-payments-relayer --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:eth-payments-relayer:prod": "npm run build:eth-payments-relayer:prod && node dist/apps/eth-payments-relayer/main",
    "start:built:chain-observer": "node ./apps/chain-observer/main.js",
    "start:built:eth-payments-relayer": "node ./apps/eth-payments-relayer/main.js",
    "start:built:platform": "npm run migrate && node ./apps/backend/main.js",
    "migrate": "npx sequelize db:migrate --config=./apps/backend/database/config/config.js --migrations-path=./apps/backend/database/migrations",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test-all-unit": "jest statistics.service.unit",
    "test-eth-payments-relayer": "jest eth-payments-relayer",
    "test-collection": "jest collection",
    "test-farm": "jest farm",
    "test-statistics": "jest statistics",
    "test-nft": "jest nft",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/cudos-aura-pool-service/test/jest-e2e.json"
  },
